{
  "visualization": {
    "plot": {
      "observation_time": 24,
      "time_increments": 200
    }
  },
  "monitor": {
    "interval": 40
  },
  "minor_planets_center": {
    "neocp_url": "https://www.minorplanetcenter.net/Extended_Files/neocp.json",
    "neo_list_keys_map": {
      "Temp_Desig": "objectName",
      "NObs": "nObs",
      "Arc": "arc",
      "H": "H",
      "Score": "neoScore",
      "R.A.": "ra",
      "Decl.": "dec",
      "V": "Vmag"
    }
  },
  "jpl": {
    "scout_api": {
      "url": "https://ssd-api.jpl.nasa.gov/scout.api",
      "eph_time_increment": 30
    },
    "neo_list_keys": {
      "objectName": {
        "display": true,
        "ext": "Temporary Designation",
        "is_score": false
      },
      "nObs": {
        "display": true,
        "ext": "Number of Observations",
        "is_score": false
      },
      "arc": {
        "display": true,
        "ext": "Observation Arc (hours)",
        "is_score": false
      },
      "rmsN": {
        "display": false,
        "ext": "rmsN",
        "is_score": false
      },
      "H": {
        "display": true,
        "ext": "Absolute Magnitude (H)",
        "is_score": false
      },
      "rating": {
        "display": false,
        "ext": "Rating",
        "is_score": false
      },
      "moid": {
        "display": true,
        "ext": "Min Orbit Intersect Dist (au)",
        "is_score": false
      },
      "caDist": {
        "display": false,
        "ext": "Close Approach Distance (au)",
        "is_score": false
      },
      "vInf": {
        "display": false,
        "ext": "Hyperbolic Excess Velocity",
        "is_score": false
      },
      "phaScore": {
        "display": false,
        "ext": "PHA Score",
        "is_score": true
      },
      "neoScore": {
        "display": true,
        "ext": "NEO Score",
        "is_score": true
      },
      "geocentricScore": {
        "display": false,
        "ext": "Geocentric Score",
        "is_score": true
      },
      "ieoScore": {
        "display": false,
        "ext": "IEO Score",
        "is_score": true
      },
      "tisserandScore": {
        "display": false,
        "ext": "Tisserand Score",
        "is_score": true
      },
      "lastRun": {
        "display": true,
        "ext": "Last Run",
        "is_score": false
      },
      "ra": {
        "display": true,
        "ext": "Current Right Ascension (ha)",
        "is_score": false
      },
      "dec": {
        "display": true,
        "ext": "Current Declination (deg)",
        "is_score": false
      },
      "elong": {
        "display": true,
        "ext": "Elongation",
        "is_score": false
      },
      "rate": {
        "display": true,
        "ext": "Rate of Precession",
        "is_score": false
      },
      "Vmag": {
        "display": true,
        "ext": "Apparent Magnitude (Vmag)",
        "is_score": false
      },
      "unc": {
        "display": true,
        "ext": "Uncertainty Current",
        "is_score": false
      },
      "uncP1": {
        "display": true,
        "ext": "Uncertainty +1 Day",
        "is_score": false
      },
      "vis": {
        "display": true,
        "ext": "Currently Visible (median orbit)",
        "is_score": false
      },
      "fullScore": {
        "display": true,
        "ext": "Observation Priority Score",
        "is_score": false
      }
    },
    "neo_list_overview_table": {
      "dra": "&Delta; Right Ascension [arcmin/h]",
      "ddec": "&Delta; Declination [arcmin/h]",
      "ra": "Right Ascension [deg]",
      "dec": "Declination [deg]",
      "vmag": "Apparent Magnitude"
    },
    "neo_list_numerically_filterable_keys": {
      "neoScore": {
        "min": 0,
        "max": 100},
      "ra": {
        "min": 0,
        "max": 24},
      "dec": {
        "min": -90,
        "max": 90},
      "unc": {
        "min": 0,
        "max": 10000},
      "Vmag": {
        "min": -50,
        "max": 50},
      "nObs": {
        "min": 0,
        "max": 1000},
      "arc": {
        "min": 0,
        "max": 1000},
      "H": {
        "min": -50,
        "max": 50},
      "rmsN": {
        "min": 0,
        "max": 50},
      "elong": {
        "min": -50,
        "max": 50},
      "rate": {
        "min": -50,
        "max": 50},
      "fullScore": {
        "min": 0,
        "max": 1},
      "moid": {
        "min": 0,
        "max": 5
      }
    }
  }
}